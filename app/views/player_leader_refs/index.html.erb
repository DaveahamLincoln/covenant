<p id="notice"><%= notice %></p>

<h1>Listing Player Leader Refs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @player_leader_refs.each do |player_leader_ref| %>
      <tr>
        <td><%= link_to 'Show', player_leader_ref %></td>
        <td><%= link_to 'Edit', edit_player_leader_ref_path(player_leader_ref) %></td>
        <td><%= link_to 'Destroy', player_leader_ref, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

